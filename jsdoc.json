{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["scripts"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "encoding": "utf8",
      "destination": "./docs",
      "recurse": true,
      "readme": "./README.md",
      "verbose": false,
      "template": "./docdash"
    }
  }
  